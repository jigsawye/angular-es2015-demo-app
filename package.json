{
  "name": "angular-es2015-demo-app",
  "version": "0.1.1",
  "description": "An Angular ES2015 demo application using Angular, AngularUI Router, Angular Translate and Angular Material",
  "scripts": {
    "clean": "rimraf dist coverage",
    "build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "build": "npm run clean && npm run build:webpack",
    "test:coverage": "karma start",
    "test": "npm run clean && npm run test:coverage",
    "lint": "eslint src",
    "api-server": "json-server -p 4000 db.json",
    "start": "node server.dev.js",
    "prod": "node server.prod.js"
  },
  "keywords": [
    "angular",
    "angularjs",
    "material",
    "webpack",
    "es2015",
    "babel"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jigsawye/angular-es2015-demo-app.git"
  },
  "author": "Evan Ye <jigsaw.ye@gmail.com> (http://github.com/jigsawye)",
  "engines": {
    "node": ">=4.0.0"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jigsawye/angular-es2015-demo-app/issues"
  },
  "homepage": "https://github.com/jigsawye/angular-es2015-demo-app",
  "dependencies": {
    "angular": "^1.4.4",
    "angular-animate": "^1.4.8",
    "angular-aria": "^1.4.8",
    "angular-material": "^1.0.1",
    "angular-translate": "^2.8.1",
    "angular-ui-router": "^0.2.15",
    "lodash": "^3.8.0"
  },
  "devDependencies": {
    "angular-mocks": "^1.3.15",
    "autoprefixer": "^6.2.3",
    "babel-core": "^6.3.26",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "browser-sync": "^2.11.0",
    "browser-sync-webpack-plugin": "^1.0.1",
    "chai": "^3.4.0",
    "cross-env": "^1.0.7",
    "css-loader": "^0.23.1",
    "eslint": "^1.6.0",
    "eslint-config-airbnb": "^3.0.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.13.3",
    "fs-walk": "0.0.1",
    "gulp": "^3.9.0",
    "gulp-rename": "^1.2.2",
    "gulp-template": "^3.1.0",
